<h2>Kitty Kollection!</h2>

<ul>
  <% @cats.each do |cat| %>
  <li>Name: <%= cat.name %></li>
  <ul>
    <li>Birthday:
      <%= cat.birth_date %>
    </li>

    <li>Sex:
      <%= cat.sex %>
    </li>

    <li>Age:
      <%= cat.age %>
    </li>
    
    <li>Color:
    <%= cat.color %>
    </li>

    <li>Description:
    <%= cat.description %>
    </li>

  </ul>
  <a href="<%= cat_url(cat) %>">Who be this?</a>
  <br>
  <% end %>
</ul>